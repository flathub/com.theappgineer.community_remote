{
  "name": "flutter",
  "buildsystem": "simple",
  "build-commands": [
    "echo $(cat flutter/bin/internal/engine.version) > flutter/bin/cache/engine-dart-sdk.stamp",
    "echo $(cat flutter/bin/internal/material_fonts.version) > flutter/bin/cache/material_fonts.stamp",
    "echo $(cat flutter/bin/internal/gradle_wrapper.version) > flutter/bin/cache/gradle_wrapper.stamp",
    "echo $(cat flutter/bin/internal/engine.version) > flutter/bin/cache/flutter_sdk.stamp",
    "echo $(cat flutter/bin/internal/engine.version) > flutter/bin/cache/font-subset.stamp",
    "echo $(cat flutter/bin/internal/engine.version) > flutter/bin/cache/linux-sdk.stamp",
    "./setup-links.sh",
    "mkdir -p /app/lib/flutter && cp -r flutter /app/lib"
  ],
  "sources": [
